<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <gradient
        android:startColor="@color/colorGreenDark"
        android:endColor="@color/colorGreenLight"
        android:angle="45">
    </gradient>
    <corners
        android:radius="150dp"/>
    <padding
        android:bottom="5pt"
        android:left="25pt"
        android:right="25pt"
        android:top="5pt">
    </padding>
    <stroke
        android:width="1dp"
        android:color="#ffffff">
    </stroke>
</shape>